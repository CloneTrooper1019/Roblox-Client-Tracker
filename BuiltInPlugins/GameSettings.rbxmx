						<Content name="LinkedSource"><null></null></Content>
						<string name="Name">ConfirmSaveDialog</string>
						<string name="ScriptGuid"></string>
						<ProtectedString name="Source"><![CDATA[local Plugin = script.Parent.Parent.Parent
local Roact = require(Plugin.Roact)
local RobloxStyle = require(Plugin.Src.Util.RobloxStyle)

local ButtonBar = require(Plugin.Src.Components.ButtonBar)

local function ConfirmSaveDialog(props)
	return Roact.createElement("Frame", {
		Size = UDim2.new(1, 0, 1, 0),
		BorderSizePixel = 0,
		BackgroundColor3 = RobloxStyle.WHITE,
	}, {
		Layout = Roact.createElement("UIListLayout", {
			FillDirection = Enum.FillDirection.Vertical,
			SortOrder = Enum.SortOrder.LayoutOrder,
		}),
		Description = Roact.createElement("TextLabel", {
			LayoutOrder = 1,
			Size = UDim2.new(1, 0, 0.5, 0),
			BackgroundTransparency = 1,
			Text = "Would you like to discard all changes?",
			Font = Enum.Font.SourceSans,
			TextColor3 = RobloxStyle.GRAY_2,
			TextSize = 24,
		}),
		DiscardChanges = Roact.createElement(ButtonBar, {
			LayoutOrder = 2,
			Buttons = {
				{Name = "No", Active = true, Value = false},
				{Name = "Yes", Default = true, Active = true, Value = true},
			},
			HorizontalAlignment = Enum.HorizontalAlignment.Center,
			ButtonClicked = function(userDiscardedChanges)
				props.OnResult(userDiscardedChanges)
			end,
		}),
	})
end

return ConfirmSaveDialog]]></ProtectedString>
