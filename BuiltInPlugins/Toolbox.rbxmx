							<Content name="LinkedSource"><null></null></Content>
							<string name="Name">Voting</string>
							<string name="ScriptGuid"></string>
							<ProtectedString name="Source"><![CDATA[local _root = script.Parent.Parent.Parent
local Roact = require(_root.Parent.libs.Roact)
local Constants = require(_root.util.Constants)
local RobloxStyle = require(_root.util.RobloxStyle)
local Padding = require(_root.util.Padding)

local Voting = Roact.PureComponent:extend("Voting")

function Voting:render()
	local layoutOrder = self.props.layoutOrder or 0

	local upVotes = self.props.upVotes
	local downVotes = self.props.downVotes
	local totalVotes = upVotes + downVotes
	local voteRatio = upVotes / totalVotes

	local votesLabel = ("%d vote%s"):format(totalVotes, (totalVotes ~= 1) and "s" or "")

	return Roact.createElement("Frame", {
		BackgroundTransparency = 1,
		LayoutOrder = layoutOrder,
		Size = UDim2.new(1, 0, 0, Constants.kAssetVotingHeight),
	}, {
		UIListLayout = Roact.createElement("UIListLayout", {
			SortOrder = Enum.SortOrder.LayoutOrder,
			HorizontalAlignment = Enum.HorizontalAlignment.Center,
		}),

		UIPadding = Padding(0, Constants.kAssetVotingInnerPadding),

		VoteBar = Roact.createElement("Frame", {
			BackgroundTransparency = 1,
			LayoutOrder = 0,
			Size = UDim2.new(1, 0, 0, Constants.kAssetVoteBarOuterHeight),
		}, {
			Background = Roact.createElement("Frame", {
				AnchorPoint = Vector2.new(0, 0.5),
				BackgroundColor3 = Color3.fromRGB(206, 100, 91),
				BorderSizePixel = 0,
				Position = UDim2.new(0, 0, 0.5, 0),
				Size = UDim2.new(1, 0, 0, Constants.kAssetVoteBarHeight),   
			}, {
				UpVotes = Roact.createElement("Frame", {
					BackgroundColor3 = Color3.fromRGB(82, 168, 70),
					BorderSizePixel = 0,
					Size = UDim2.new(voteRatio, 0, 1, 0),
				}),
			}),
		}),

		VoteCount = Roact.createElement("TextLabel", {
			BackgroundTransparency = 1,
			LayoutOrder = 1,
			Size = UDim2.new(1, 0, 0, Constants.kAssetVoteCountHeight),
			Text = votesLabel,
			TextColor3 = RobloxStyle.kGray2,
			Font = RobloxStyle.kFont,
			TextSize = Constants.kAssetVoteCountFontSize,
		}),
	}),
end

return Voting]]></ProtectedString>
							<BinaryString name="Tags"></BinaryString>
