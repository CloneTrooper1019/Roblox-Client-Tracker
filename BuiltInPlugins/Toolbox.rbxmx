						<Content name="LinkedSource"><null></null></Content>
						<string name="Name">RobloxScrollingFrame</string>
						<string name="ScriptGuid"></string>
						<ProtectedString name="Source"><![CDATA[local _root = script.Parent.Parent
local Roact = require(_root.Parent.libs.Roact)
local Constants = require(_root.util.Constants)
local RobloxStyle = require(_root.util.RobloxStyle)

local RobloxScrollingFrame = Roact.PureComponent:extend("RobloxScrollingFrame")

function RobloxScrollingFrame:render()
	local position = self.props.Position or UDim2.new(0, 0, 0, 0)
	local size = self.props.Size or UDim2.new(1, 0, 1, 0)
	local canvasSize = self.props.CanvasSize or UDim2.new(0, 0, 1, 0)
	local zindex = self.props.ZIndex or 1

	return Roact.createElement("Frame", {
		Position = position,
		Size = size,
		BackgroundTransparency = 1,
	}, {
		ScrollBarBackground = Roact.createElement("Frame", {
			AnchorPoint = Vector2.new(1, 0),
			Position = UDim2.new(1, 0, 0, 0),
			Size = UDim2.new(0, RobloxStyle.kScrollBarBackgroundThickness, 1, 0),
			BorderSizePixel = 0,
			BackgroundColor3 = RobloxStyle.kGray6,
		}),

		ScrollingFrame = Roact.createElement("ScrollingFrame", {
			Position = UDim2.new(0, 0, 0, 2),
			Size = UDim2.new(1, -RobloxStyle.kScrollBarPadding, 1, -4),
			CanvasSize = canvasSize,
			BorderSizePixel = 0,
			BackgroundTransparency = 1,
			ScrollBarThickness = RobloxStyle.kScrollBarThickness,
			ZIndex = 2,

			TopImage = RobloxStyle.kScrollBarTop,
			MidImage = RobloxStyle.kScrollBarMiddle,
			BottomImage = RobloxStyle.kScrollBarBottom,
			ScrollingEnabled = true,
			ElasticBehavior = Enum.ElasticBehavior.Always,
			ScrollingDirection = Enum.ScrollingDirection.Y,
		}, self.props[Roact.Children]),
	})
end

return RobloxScrollingFrame]]></ProtectedString>
