					<Item class="ModuleScript" referent="RBXA47D2CD54C454F369A63446E89A8A895">
						<Properties>
							<Content name="LinkedSource"><null></null></Content>
							<string name="Name">Asset</string>
							<string name="ScriptGuid"></string>
							<ProtectedString name="Source"><![CDATA[local _root = script.Parent.Parent.Parent
local Roact = require(_root.Parent.libs.Roact)
local Constants = require(_root.util.Constants)
local RobloxStyle = require(_root.util.RobloxStyle)
local Padding = require(_root.util.Padding)

local AssetIcon = require(script.Parent.AssetIcon)
local AssetName = require(script.Parent.AssetName)
local AssetCreatorName = require(script.Parent.AssetCreatorName)
local Voting = require(script.Parent.Voting)

local Asset = Roact.Component:extend("Asset")

function Asset:shouldUpdate(nextProps, nextState)
	-- We only want to rerender the asset if it has gone from being not hovered to hovered, or hovered to not-hovered, or if the background has changed
	-- Otherwise when hovering elements in the toolbox, every asset was being rerendered twice
	-- Once to set unhovered on all to false, then again to set hovered on one of them
	-- That made the toolbox feel really slow
	-- This checks makes only the 2 affected assets rerender
	-- As more gets added to the asset, this check will need to be expanded
	return (self.props.isHovered ~= nextProps.isHovered)
		or (self.props.backgroundIndex ~= nextProps.backgroundIndex)
end

function Asset:render()
	local assetData = self.props.asset

	local asset = assetData.Asset
	local id = asset.Id
	local isEndorsed = asset.IsEndorsed
	local assetName = asset.Name

	local creator = assetData.Creator
	local creatorName = creator.Name

	-- TODO: Handle voting.ShowVotes
	-- And when the user has inserted an asset, it shows the vote up/down buttons
	local voting = assetData.Voting
	local upVotes = voting.UpVotes
	local downVotes = voting.DownVotes

	local layoutOrder = self.props.layoutOrder
	local isHovered = self.props.isHovered
	local backgroundIndex = self.props.backgroundIndex

	return Roact.createElement("Frame", {
		Position = UDim2.new(0, 0, 0, 0),
		Size = UDim2.new(1, 0, 1, 0),
		LayoutOrder = layoutOrder,
		BackgroundTransparency = 1,

		-- Need to raise the container up over the other assets to show the outline correctly
		ZIndex = isHovered and 2 or 1,

		[Roact.Event.MouseEnter] = function (rbx, x, y)
			self.props.onAssetHovered(layoutOrder)
		end,
		[Roact.Event.InputChanged] = function (rbx, input)
			if (input.UserInputType == Enum.UserInputType.MouseMovement) then
				self.props.onAssetHovered(layoutOrder)
			end
		end,

		[Roact.Event.InputBegan] = function (rbx, input)
			if (input.UserInputType == Enum.UserInputType.MouseButton1) then
				-- TODO: Insertion on click or drag
				print(("MouseClick\tid: %d \tname: \"%s\" \tby \"%s\""):format(id, assetName, creatorName))
			end
			-- TODO: On right click, show a context menu
		end,
	}, {
		Outline = Roact.createElement("Frame", {
			AnchorPoint = Vector2.new(0.5, 0),
			BackgroundTransparency = isHovered and Constants.kAssetOutlineHoveredTransparency or 1,
			BackgroundColor3 = RobloxStyle.kWhite,
			BorderColor3 = RobloxStyle.kGray3,
			BorderSizePixel = 1,
			Position = UDim2.new(0.5, 0, 0, -Constants.kAssetOutlinePadding),
			Size = UDim2.new(1, 2 * Constants.kAssetOutlinePadding, 1, Constants.kAssetOutlineExtraHeight),
			ZIndex = -1,

			[Roact.Event.MouseLeave] = function (rbx, x, y)
				self.props.onAssetHoverEnded(layoutOrder)
			end,
		}),

		InnerFrame = Roact.createElement("Frame", {
			BackgroundTransparency = 1,
			Position = UDim2.new(0, 0, 0, 0),
			Size = UDim2.new(1, 0, 1, 0),
			ClipsDescendants = not isHovered,
		}, {
			UIListLayout = Roact.createElement("UIListLayout", {
				Padding = UDim.new(0, Constants.kAssetInnerPadding),
				SortOrder = Enum.SortOrder.LayoutOrder,
				HorizontalAlignment = Enum.HorizontalAlignment.Center,
			}),

			IconBackground = Roact.createElement(AssetIcon, {
				layoutOrder = 0,
				assetId = id,
				isEndorsed = isEndorsed,
				backgroundIndex = backgroundIndex,
			}),

			AssetName = Roact.createElement(AssetName, {
				layoutOrder = 1,
				assetName = assetName,
			}),

			CreatorName = Roact.createElement(AssetCreatorName, {
				layoutOrder = 2,
				creatorName = creatorName,
			}),

			Voting = Roact.createElement(Voting, {
				layoutOrder = 3,
				upVotes = upVotes,
				downVotes = downVotes,
			}),
		}),
	})
end

return Asset]]></ProtectedString>
