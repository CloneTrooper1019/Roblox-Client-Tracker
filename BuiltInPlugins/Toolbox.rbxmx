						<Content name="LinkedSource"><null></null></Content>
						<string name="Name">FFlag</string>
						<string name="ScriptGuid"></string>
						<ProtectedString name="Source"><![CDATA[--[[
Helper object for querying and caching flag state
Usage:
local FFlag = require(util.FFlag)

if (FFlag.FlagName) then
	-- Flag on state
else
	-- Flag off state
end
]]

return setmetatable({ }, {
	__index = function (self, flagName)
		-- By default assume a flag is false
		local isEnabled = false

		-- Safely get the flag value
		local success, flagOn = pcall(function ()
			return settings():GetFFlag(flagName)
		end)

		-- If the flag exists then use its value
		if (success) then
			isEnabled = flagOn
		end

		-- Cache the flag value 
		rawset(self, flagName, isEnabled)

		return isEnabled
	end,
})]]></ProtectedString>
