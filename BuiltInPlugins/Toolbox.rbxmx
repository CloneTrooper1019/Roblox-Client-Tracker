							<string name="ScriptGuid"></string>
							<ProtectedString name="Source"><![CDATA[local _root = script.Parent.Parent.Parent
local Roact = require(_root.Parent.libs.Roact)
local Constants = require(_root.util.Constants)

local AssetIcon = Roact.PureComponent:extend("AssetIcon")

function getThumbnailUrlForAsset(assetId, width, height)
	width = width or 420
	height = height or 420
	return "https://assetgame.roblox.com/asset-thumbnail/image?assetId=" .. tostring(assetId) .. "&width=" .. tostring(width) .. "&height=" .. tostring(height)
end

function AssetIcon:init(props)
	self.thumbnailUrl = getThumbnailUrlForAsset(props.assetId,
		Constants.kAssetThumbnailRequestedImageSize,
		Constants.kAssetThumbnailRequestedImageSize)
end

function AssetIcon:render()
	local layoutOrder = self.props.layoutOrder or 0

	local thumbnailUrl = self.thumbnailUrl
	local backgroundIndex = self.props.backgroundIndex
	local isEndorsed = self.props.isEndorsed

	-- For "none" background image
	local tileCount = 8

	return Roact.createElement("ImageLabel", {
		-- TODO: Tidy up handling of backgrounds, this assumes too much
		BackgroundColor3 = ((backgroundIndex == 1) or (backgroundIndex == 3)) and Color3.fromRGB(255, 255, 255) or Color3.fromRGB(0, 0, 0),
		Image = (backgroundIndex == 3) and Constants.kNoBackgroundIcon or "",
		BorderSizePixel = 0,
		Size = UDim2.new(1, 0, 1, 0),
		SizeConstraint = Enum.SizeConstraint.RelativeXX,
		ScaleType = Enum.ScaleType.Tile,
		TileSize = UDim2.new(1 / tileCount, 0, 1 / tileCount, 0),
		ZIndex = 1,
		LayoutOrder = layoutOrder,
	}, {
		Image = Roact.createElement("ImageLabel", {
			BackgroundTransparency = 1,
			Size = UDim2.new(1, 0, 1, 0),
			Image = thumbnailUrl,
		}),

		Badge = Roact.createElement("ImageLabel", {
			AnchorPoint = Vector2.new(1, 1),
			BackgroundTransparency = 1,
			Position = UDim2.new(0.92, 0, 1.06, 0),
			Size = UDim2.new(0, Constants.kAssetBadgeSize, 0, Constants.kAssetBadgeSize),
			Image = Constants.kEndorsedBadge,
			Visible = isEndorsed,
			ZIndex = 2,
		}),
	}),
end

return AssetIcon]]></ProtectedString>
